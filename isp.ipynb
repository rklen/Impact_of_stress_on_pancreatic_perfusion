    "import pydicom\n",
    "import nibabel as nib\n",
    "import os\n",
    "import cv2\n",
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "from scipy.ndimage.measurements import label\n",
    "from matplotlib.collections import LineCollection"
